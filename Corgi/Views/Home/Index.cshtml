                    <div class="panel-heading" role="tab" id="headingTest">
                        <h4 class="panel-title">
                            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTest" aria-expanded="true" aria-controls="collapseTest">
                                <i class="more-less glyphicon glyphicon-plus"></i>
                                <h2><center>@newsfeed.StoryName</center></h2>
                            </a>
                        </h4>
                    </div>

                    <div id="collapseTest" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTest">
                        @foreach (var article in newsfeed.Articles)
                        {
                        <div class="panel-body">
                            <center><a href="@article.Url">@newsfeed.StoryName</a></center>
